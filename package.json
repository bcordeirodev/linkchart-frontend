    "dev": "NODE_ENV=development next dev --turbo",
    "build": "next build",
    "start": "next start",
    "start:prod": "NODE_ENV=production next start",
    "lint": "eslint ./src --config ./eslint.config.mjs",
    "lint:fix": "eslint --fix ./src --config ./eslint.config.mjs",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "audit": "npm audit --production",
    "validate": "node docker/scripts/validate-structure.js",
    "validate:fix": "npm run lint:fix && npm run format && npm run validate",
    "fix:client": "node docker/scripts/fix-client-components.js",
    "fix:all": "npm run fix:client && npm run lint:fix && npm run format && npm run validate",
    "cleanup": "node docker/scripts/cleanup.js",
    "docker:build": "./docker/scripts/build-docker.sh",
    "docker:test": "./docker/scripts/test-docker.sh",
    "docker:all": "npm run cleanup && npm run docker:build && npm run docker:test",
    "docker:dev": "docker-compose -f docker/development/docker-compose.yml up",
    "docker:prod": "docker-compose -f docker/production/docker-compose.yml up -d"
  },
  "engines": {
    "node": ">=22.12.0",
    "npm": ">=10.9.0"
